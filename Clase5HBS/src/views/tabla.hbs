<form method="get" action="/">
    <button type="submit">Formulario</button>
</form>
<h2>Productos Guardados</h2>

<table class="table table-striped table-dark">
  <tr>
    <th>Nombre</th>
    <th>Precio</th> 
    <th>Thumbnail</th>
  </tr>
  {{#each customerFile}}
  <tr>
    <td>{{this.Nombre}}</td>
    <td>{{this.Precio}}</td> 
    <td><img src="./{{this.url}}" class="img-fluid" alt="Responsive image"></img></td>
  </tr>
  {{/each}}
</table>